{
    "builds": [
      {
        "src": "hng_stage2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "Python3.12",
          "buildCommand": "chmod +x build_file.sh && ./build_file.sh"
        }
      }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "hng_stage2/wsgi.py"
      }
    ],
    "installCommand": "pip install pipenv && pipenv install --deploy --ignore-pipfile"
}

