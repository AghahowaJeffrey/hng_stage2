{
    "builds": [
      {
        "src": "hng_stage2/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "Python3.12"
        }
      },
      {
        "src": "build_file.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],

    "routes": [
      {
        "src": "/(.*)",
        "dest": "hng_stage2/wsgi.py"
      }
    ]
}